<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane fx:id="border" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hotPotato.WinScreenController">
   <top>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="The End!" BorderPane.alignment="CENTER">
         <font>
            <Font size="34.0" />
         </font></Text>
   </top>
   <right>
      <VBox prefHeight="171.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Text fx:id="first" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
               <font>
                  <Font size="20.0" />
               </font>
               <VBox.margin>
                  <Insets left="10.0" top="20.0" />
               </VBox.margin></Text>
            <Text fx:id="second" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
               <font>
                  <Font size="20.0" />
               </font>
               <VBox.margin>
                  <Insets left="10.0" top="30.0" />
               </VBox.margin></Text>
            <Text fx:id="third" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
               <font>
                  <Font size="20.0" />
               </font>
               <VBox.margin>
                  <Insets left="10.0" top="30.0" />
               </VBox.margin></Text>
         </children>
      </VBox>
   </right>
   <left>
      <VBox prefHeight="130.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="1st">
               <font>
                  <Font size="25.0" />
               </font>
               <VBox.margin>
                  <Insets left="30.0" top="18.0" />
               </VBox.margin></Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="2nd">
               <font>
                  <Font size="25.0" />
               </font>
               <VBox.margin>
                  <Insets left="30.0" top="22.0" />
               </VBox.margin></Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="3rd">
               <font>
                  <Font size="25.0" />
               </font>
               <VBox.margin>
                  <Insets left="30.0" top="22.0" />
               </VBox.margin></Text>
         </children>
      </VBox>
   </left>
   <bottom>
      <HBox prefHeight="32.0" prefWidth="221.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#buttonClickHandler" prefHeight="25.0" prefWidth="75.0" text="Quit">
               <HBox.margin>
                  <Insets left="20.0" right="25.0" top="20.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#buttonClickHandler" prefHeight="25.0" prefWidth="75.0" text="Start Over">
               <HBox.margin>
                  <Insets bottom="10.0" left="5.0" top="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </bottom>
</BorderPane>
